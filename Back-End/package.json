{
  "name": "back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel routes",
    "start": "nodemon --exec babel-node ./bin/www",
    "test": "mocha --require @babel/register --recursive",
    "testCMD": "mocha --require @babel/register --recursive -w",
    "cover": "cross-env NODE_ENV=test nyc mocha --require @babel/register --recursive",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "app.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "minimist": "^1.2.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.19.0",
    "shelljs": "^0.8.3",
    "vinyl-ftp": "^0.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-plugin-istanbul": "^5.1.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "gulp": "^4.0.1",
    "mocha": "^6.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.0.0",
    "supertest": "^4.0.2"
  }
}
